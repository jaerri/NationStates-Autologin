{
    "manifest_version": 3,
    "name": "NationStates Autologin",
    "description": "Test",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": [
        "notifications",
        "storage"
    ],
    "options_page": "options.html"
}